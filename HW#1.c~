#include <stdio.h>

int mystrlen(char s[]){
  int i = 0;
  for(;s[i];i++);
  return i;
}

char* mystrcpy(char goal[], char s[]){
  int n = 0;
  for (;n<mystrlen(s);n++){
    //  goal[n] = char[n];
  }
  return &goal;
}

